{
  "name": "fantasyelim",
  "version": "1.0.0",
  "description": "Fantasy Football Eliminator",
  "main": "server.js",
  "engines": {
    "node": "8.11.3"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore client/\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "firebase": "^5.11.1",
    "if-env": "^1.0.4",
    "moment": "^2.24.0",
    "mongoose": "^5.6.4",
    "styled-components": "^4.3.2",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fantasyelim/fantasyelim.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/fantasyelim/fantasyelim/issues"
  },
  "homepage": "https://github.com/fantasyelim/fantasyelim#readme"
}
